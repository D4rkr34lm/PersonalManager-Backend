{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-non-null-assertion": "off",
            "@typescript-eslint/no-misused-promises": [
            "error", {
                "checksVoidReturn": false
            }
        ]
    }
}
